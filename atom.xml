---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>this is github.com/CraigCottingham</title>
  <!-- subtitle type="html" -->
  <id>http://craigcottingham.github.com/</id>
  <updated>{{site.time | date_to_xmlschema }}</updated>
  <link rel="self" type="application/atom+xml" href="http://craigcottingham.github.com/atom.xml" />
  <link rel="alternate" type="text/html" hreflang="en" href="http://craigcottingham.github.com/" />
  <rights>Copyright (c) 2010-2011 Craig S. Cottingham</rights>
  <!-- generator -->
  <author>
    <name>Craig S. Cottingham</name>
    <email>craig.cottingham@gmail.com</email>
  </author>
  {% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link rel="alternate" type="text/html" href="http://craigcottingham.github.com{{ post.id }}" />
    <id>http://craigcottingham.github.com{{ post.id }}</id>
    <!-- published -->
    <updated>{{post.date | date_to_xmlschema }}</updated>
    <author>
      <name>Craig S. Cottingham</name>
      <email>craig.cottingham@gmail.com</email>
    </author>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>